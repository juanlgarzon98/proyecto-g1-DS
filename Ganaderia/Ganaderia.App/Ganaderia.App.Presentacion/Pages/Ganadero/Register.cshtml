@page
@model Ganaderia.App.Presentacion.Pages.RegisterModel
@{
}

    <div class="register">
        <h1 class="tituloPagina">Agregar nuevo Ganadero</h1>
    <form>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="inputEmail4">Nombres</label>
                <input type="text" class="form-control" id="nombreG" placeholder="Nombres">
            </div>
            <div class="form-group col-md-6">
                <label for="inputPassword4">Apellidos</label>
                <input type="email" class="form-control" id="apellidosG" placeholder="Apellidos">
            </div>
        </div>
        <div class="form-group">
            <label for="inputAddress">Numero Telefono</label>
            <input type="text" class="form-control" id="telefonoG" placeholder="0000000">
        </div>
        <div class="form-group">
            <label for="inputAddress2">Correo</label>
            <input type="email" class="form-control" id="correoG" placeholder="example@gmail.com">
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="inputCity">Contraseña</label>
                <input type="password" class="form-control" id="contrasenaG">
            </div>           
        </div>
      
        <button type="submit" class="btn btn-primary">Guardar</button>
    </form>
    </div>
